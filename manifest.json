{
  "name": "Summarizer",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "tabs", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dummy.js"]
    }
  ],
  "action": {
    "default_title": "Summarizer",
    "default_popup": "popup.html",
    "default_area": "navbar"
  },
  "icons": {
    "128": "favicon.png"
  }
}
